<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <template id="snippets" inherit_id="website.snippets">
        <xpath expr="//div[@id='snippet_effect']/div[hasclass('o_panel_body')]" position="inside">
            <t t-snippet="pinakes_website.s_product_public_category_tags" string="Rubriek Labels" t-thumbnail="/pinakes_website/static/src/img/(futurePreviewDuSnippet)">
                <keywords>categories, dynamic</keywords>
            </t>
        </xpath>
    </template>
</odoo>