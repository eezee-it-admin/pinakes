<?xml version="1.0" encoding="utf-8"?>
<odoo>

    <record id="view_politeia_publication_lang_tree" model="ir.ui.view">
        <field name="name">politeia.publication.lang.tree</field>
        <field name="model">publication.lang</field>
        <field name="arch" type="xml">
            <tree editable="bottom">
                <field name="name"/>
            </tree>
        </field>
    </record>

    <record id="action_politeia_publication_lang" model="ir.actions.act_window">
        <field name="name">Publication Language</field>
        <field name="res_model">publication.lang</field>
        <field name="view_mode">tree</field>
    </record>

</odoo>
